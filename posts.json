---
layout: none 
permalink: /posts.json
---
[
{% for p in site.posts %}
  {"title": {{ p.title | jsonify }},
   "url": {{ p.url | relative_url | jsonify }},
   "date": "{{ p.date | date_to_xmlschema }}",
   "tags": {{ p.tags | jsonify }},
   "category": {{ p.category | jsonify }},
   "excerpt": {{ p.excerpt | strip_html | jsonify }}}
  {% unless forloop.last %},{% endunless %}
{% endfor %}
]
